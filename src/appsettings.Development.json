{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning",
            "System.Net.Http": "Error",
            "Microsoft.AspNetCore.Routing.EndpointMiddleware": "None",
            "Microsoft.AspNetCore.Hosting.Diagnostics": "None",
            "Microsoft.Azure.SpaceFx": "Debug",
